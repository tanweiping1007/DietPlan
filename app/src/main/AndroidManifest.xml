                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".DietPackageDetailActivity"
            android:label="@string/title_activity_diet_package_detail" >
        </activity>
        <activity
